        "@angular/flex-layout": "13.0.0-beta.38",
        "@angular/forms": "13.3.11",
        "@angular/material": "13.3.9",
        "@angular/platform-browser": "13.3.11",
        "@angular/platform-browser-dynamic": "13.3.11",
        "@angular/router": "13.3.11",
        "@swimlane/ngx-charts": "20.1.0",
        "android-versions": "1.5.0",
        "chart.js": "3.8.0",
        "core-js": "3.8.1",
        "firebase": "9.8.2",
        "hammerjs": "2.0.8",
        "localforage": "1.10.0",
        "moment": "2.29.3",
        "neutrinos-module": "0.0.70-beta.2",
        "neutrinos-oauth-client": "1.2.5-beta.1",
        "ng2-charts": "3.0.11",
        "rxjs": "7.5.5",
        "signature_pad": "4.0.7",
        "tslib": "2.4.0",
        "zone.js": "0.11.4"
    },
